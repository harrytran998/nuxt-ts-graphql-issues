{
  "name": "xxxx",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "codegen": "apollo client:codegen --endpoint=http://localhost:8080/graphql --includes=src/graphql/**/*.gql --no-addTypename --target=typescript --tagName=gql --globalTypesFile=src/graphql/types/GraphTypes.ts --outputFlat src/graphql/types",
    "test:e2e": "wdio wdio.conf.js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@chakra-ui/nuxt": "^0.1.0",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/apollo": "^4.0.1-rc.4",
    "@nuxtjs/composition-api": "^0.13.5",
    "@nuxtjs/emotion": "^0.0.1",
    "@nuxtjs/pwa": "^3.0.2",
    "@vue/apollo-composable": "^4.0.0-alpha.12",
    "apollo-cache-inmemory": "^1.6.6",
    "core-js": "^3.6.5",
    "graphql": "^15.3.0",
    "nuxt": "^2.14.7",
    "nuxt-multi-tenancy-module": "^0.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@wdio/cli": "^6.5.2",
    "@wdio/local-runner": "^6.5.2",
    "@wdio/mocha-framework": "^6.5.0",
    "@wdio/spec-reporter": "^6.4.7",
    "@wdio/sync": "^6.5.0",
    "apollo": "^2.31.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-wdio": "^6.0.12",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "webdriverio": "^6.5.2"
  }
}
